{
  "name": "minvws/laravel-sql-exporter",
  "description": "Generic audit logger",
  "keywords": [
    "laravel",
    "sql",
    "export"
  ],
  "type": "library",
  "license": "EUPL",
  "authors": [
    {
      "name": "Ministerie van Volksgezondheid, Welzijn en Sport"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10",
    "nesbot/carbon": "^2.66"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^8.0",
    "vimeo/psalm": "^5.8",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7",
    "slevomat/coding-standard": "^8.8"
  },
  "autoload": {
    "psr-4": {
      "MinVWS\\SqlExporter\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\SqlExporter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
        "vendor/bin/phpcs",
        "vendor/bin/phpunit",
        "vendor/bin/phpstan analyse",
        "vendor/bin/psalm"
    ]
  },
  "config": {
    "lock": false,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinVWS\\SqlExporter\\SqlExporterServiceProvider"
      ]
    }
  }
}
