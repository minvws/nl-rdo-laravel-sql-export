{
  "name": "minvws/sql-exporter",
  "description": "Generic audit logger",
  "keywords": [
    "laravel",
    "sql",
    "export"
  ],
  "type": "library",
  "license": "EUPL",
  "authors": [
    {
      "name": "Gerbrand Bosch",
      "email": "gerbrandb@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.17",
    "nesbot/carbon": "^2.66"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^8.15"
  },
  "autoload": {
    "psr-4": {
      "MinVWS\\SqlExporter\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\SqlExporter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
        "vendor/bin/phpcs",
        "vendor/bin/phpunit",
        "vendor/bin/phpstan analyse",
        "vendor/bin/psalm"
    ]
  },
  "config": {
  }
}
